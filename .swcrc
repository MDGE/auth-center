{
    "jsc": {
        "target":"es2021",
        "parser": {
            "syntax": "typescript", 
            "tsx":true,
            "dynamicImport":true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true,
            "react": {
              "runtime": "automatic",
              "throwIfNamespace": true,
              "useBuiltins": true
            }
        }
    }
}
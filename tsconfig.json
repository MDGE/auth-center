{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "incremental": true,
    "outDir": "build/lib",
    "target": "esnext",
    "module": "commonjs",
    "lib": ["dom", "esnext"],
    "sourceMap": true,
    "importHelpers": true,
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowJs": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true,
    "rootDir": ".",
    "baseUrl": "src/",
    "paths": {
      "common/*":["common/*"],
      "components/*": ["components/*"],
      "assets/*": ["assets/*"],
      "pages/*": ["pages/*"],
      "store/*":["store/*"],
      "routes/*":["routes/*"],
      "utils/*":["utils/*"],
      "menus/*":["menus/*"],
      "sdk/*":["sdk/*"],
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules","axios"]
}
  